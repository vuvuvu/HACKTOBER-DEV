{
  "name": "hacktober-dev",
  "version": "0.1.0",
  "description": "CLI tool to measure your hackiness level through comprehensive Git repository analysis",
  "main": "dist/cli.js",
  "bin": {
    "hacktober-dev": "dist/cli.js"
  },
  "files": [
    "dist",
    "data",
    "schema"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts",
    "start": "node dist/cli.js",
    "test": "node dist/cli.js --test",
    "validate": "node scripts/validate-definitions.js",
    "prepublishOnly": "npm run build && npm run validate",
    "version": "npm run build && git add dist/",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "simple-git": "^3.20.0",
    "cli-table3": "^0.6.3",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "hacktoberfest",
    "cli",
    "git",
    "analytics",
    "developer habits",
    "metrics",
    "achievements"
  ],
  "author": "vuvuvu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuvuvu/hacktober-dev.git"
  },
  "bugs": {
    "url": "https://github.com/vuvuvu/hacktober-dev/issues"
  },
  "homepage": "https://github.com/vuvuvu/hacktober-dev#readme"
}